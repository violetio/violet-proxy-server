{
    "compilerOptions": {
        "module": "esnext",
        "esModuleInterop": true,
        "target": "es5",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/*"
            ],
            "@/middleware/*": ["src/middleware/*"],
            "@/strings/*": ["src/strings/*"],
        }
    },
    "include": [
        "src/**/*"
    ]
}